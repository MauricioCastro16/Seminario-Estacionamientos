@model estacionamientos.Models.Duenio
@{
    ViewData["Title"] = "Detalles del Dueño";
}
<h1>@ViewData["Title"]</h1>
<hr />

<dl class="row">
    <dt class="col-sm-3">Id</dt><dd class="col-sm-9">@Model.UsuNU</dd>
    <dt class="col-sm-3">Nombre y Apellido</dt><dd class="col-sm-9">@Model.UsuNyA</dd>
    <dt class="col-sm-3">Email</dt><dd class="col-sm-9">@Model.UsuEmail</dd>
    <dt class="col-sm-3">Teléfono</dt><dd class="col-sm-9">@Model.UsuNumTel</dd>
    <dt class="col-sm-3">CUIT</dt><dd class="col-sm-9">@Model.DueCuit</dd>
</dl>

<div class="d-flex justify-content-between align-items-center mb-3">
    <div class="d-flex gap-2">
        <a asp-action="Edit" asp-route-id="@Model.UsuNU" class="btn btn-warning">Editar</a>
    </div>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</div>
